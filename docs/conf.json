{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "src",
            "tst/e2e",
            "tst/unit"
        ],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true
    }
}
